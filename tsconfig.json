{
    "compilerOptions": {
      "module": "ESNext",
      "target": "es5",
      "lib": ["es6", "dom", "ESNext"],
      "sourceMap": true,
      "allowJs": false,
      "jsx": "react",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true
    },
    "include": [
        "src"
    ],
    "exclude": [
      "node_modules",
      "build",
    ]
  }